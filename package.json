{
  "name": "prettier-parallel",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/sosukesuzuki/prettier-parallel.git"
  },
  "license": "MIT",
  "author": "Sosuke Suzuki <aosukeke@gmail.com>",
  "files": [
    "build/**/*",
    "bin/**/*",
    "!src/**/*"
  ],
  "bin": "./bin/prettier-prallel.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "build:watch": "yarn build --watch",
    "clean": "rimraf ./build",
    "format": "prettier --write \"**/*.{ts,md,json}\"",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "colors": "^1.3.3",
    "glob": "^7.1.4",
    "ignore": "^5.1.2"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/prettier": "^1.18.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "prettier": ">=1.8.0"
  }
}
